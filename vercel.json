{
  "version": 2,
  "builds": [
    {
      "src": "api/extension/**/*.ts",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/extension/sign-in",
      "dest": "/api/extension/sign-in"
    },
    {
      "src": "/api/extension/auth/callback",
      "dest": "/api/extension/auth/callback"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}