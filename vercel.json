{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/extension/sign-in",
      "destination": "/api/extension/sign-in"
    },
    {
      "source": "/extension/auth/callback",
      "destination": "/api/extension/auth/callback"
    },
    {
      "source": "/api/stripe/:action",
      "destination": "/api/stripe/[action]"
    },
    {
      "source": "/api/extension/auth/:action",
      "destination": "/api/extension/auth/[action]"
    },
    {
      "source": "/api/user/:action",
      "destination": "/api/user/[action]"
    },
    {
      "source": "/api/billing/credit-purchase",
      "destination": "/api/billing/credit-purchase"
    },
    {
      "source": "/api/clerk/webhooks",
      "destination": "/api/clerk/webhooks"
    },
    {
      "source": "/api/starter/process-signup",
      "destination": "/api/starter/process-signup"
    },
    {
      "source": "/api/stripe/webhooks",
      "destination": "/api/stripe/webhooks"
    },
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/clerk/webhooks.ts": {
      "maxDuration": 10,
      "memory": 512
    },
    "api/billing/credit-purchase.js": {
      "maxDuration": 30,
      "memory": 512
    },
    "api/stripe/[action].js": {
      "maxDuration": 30,
      "memory": 512
    },
    "api/extension/auth/[action].ts": {
      "maxDuration": 10
    },
    "api/user/[action].ts": {
      "maxDuration": 10
    },
    "api/starter/process-signup.js": {
      "maxDuration": 30,
      "memory": 512
    },
    "api/stripe/webhooks.js": {
      "maxDuration": 30,
      "memory": 512
    }
  },
  "crons": [],
  "regions": ["iad1"],
  "headers": [
    {
      "source": "/api/auth/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    },
    {
      "source": "/api/extension/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    },
    {
      "source": "/api/stripe/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    },
    {
      "source": "/api/user/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)\\.js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript; charset=utf-8"
        }
      ]
    },
    {
      "source": "/(.*)\\.css",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/css; charset=utf-8"
        }
      ]
    },
    {
      "source": "/(.*)\\.mjs",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript; charset=utf-8"
        }
      ]
    }
  ]
}